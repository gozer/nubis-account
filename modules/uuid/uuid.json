{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Parameters": {
    "LambdaUUIDArn": {
      "Description": "ARN of a UUID generating function",
      "Type": "String"
    }
  },
  "Resources": {
    "UUID": {
      "Type": "Custom::UUID",
      "Properties": {
        "ServiceToken": {
          "Ref": "LambdaUUIDArn"
        }
      }
    }
  },
  "Outputs": {
    "UUID": {
      "Value": {
        "Fn::GetAtt": [
          "UUID",
          "uuid"
        ]
      }
    }
  }
}
